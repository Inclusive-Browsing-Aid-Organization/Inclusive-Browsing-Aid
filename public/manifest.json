{
    "manifest_version": 3,
    "name": "Inclusive Browsing Aid",
    "version": "1.0",
    "description": "A simple Chrome extension.",
    "action" : {
        "default_popup": "index.html",
        "default_title": "main dashboard"
    },
    "permissions":[
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "https://*/",
        "http://*/"
    ]
  }